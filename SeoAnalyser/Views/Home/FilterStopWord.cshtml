@model SeoAnalyser.Models.StopWords

@{
    ViewBag.Title = "FilterStopWord";
    var url = ViewBag.Url;
    var data = Model;
}

@if (!string.IsNullOrEmpty(url))
{
    <h3>Count of stop words when you select @url  </h3>
}
else
{
    <h3>The result of stop words with count</h3>
}
<br />

<div id="exTab2" class="container">
    <ul class="nav nav-tabs">
        <li class="active">
            <a href="#1" data-toggle="tab">Body</a>
        </li>
        <li>
            <a href="#2" data-toggle="tab">Link</a>
        </li>
     
    </ul>

    <div class="tab-content ">
        <div class="tab-pane active" id="1">
           @Html.Partial("FilterStopWordList", data.Body)
        </div>
        <div class="tab-pane" id="2">
            @Html.Partial("FilterStopWordList", data.Link)
        </div>
       
    </div>
</div>



